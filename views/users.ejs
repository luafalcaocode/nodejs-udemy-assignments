<%- include('layout/begin.ejs') %>

<main>
    <h1>Users</h1>
    <p>Welcome to the user page</p>

    <strong>Current User: </strong>



    <% if (users.length > 0) {  %>
       <ul>
            <% for (const user of  users) { %>
                <li class="item-list"><%= user.name %></li>
            <% } %>
        </ul>
    <% } else { %>
        <h1>No users</h1>
    <% } %>
</main>


<%- include('layout/end.ejs') %>